\mode<presentation>

\definecolor{mrclightblue}{RGB}{234,239,243}
\definecolor{mrcblue}{RGB}{33,103, 126}
\definecolor{mrcorange}{RGB}{208,114,50}
\definecolor{mrcyellow}{RGB}{255,219,0}
\definecolor{mrcgreen}{RGB}{181,211,52}
\definecolor{mrcpurple}{RGB}{106,59,119}
\definecolor{mrcred}{RGB}{130,47,90} % "red"
\definecolor{mrcgray}{RGB}{138,121,103}

\setbeamercolor{background canvas}{bg=mrclightblue}
\setbeamercolor{boxtitles}{bg=white,fg=mrcblue}

\setbeamercolor{block title}{fg=white}
\setbeamerfont{block title}{size=\huge}

\setbeamercolor{section}{fg=mrcorange}
\setbeamerfont{caption}{shape=\itshape,size=\normalsize}
\setbeamerfont{caption name}{shape=\itshape}
\setbeamercolor{caption name}{fg=black}

\setbeamerfont{itemize/enumerate subbody}{size=\normalsize}

\setbeamercolor{itemize subitem}{fg=black}
\setbeamercolor{enumerate subitem}{fg=black}
\setbeamercolor{itemize item}{fg=black}
\setbeamercolor{enumerate item}{fg=black}
\setbeamertemplate{itemize item}{\raisebox{-0.9ex}{\scalebox{3}{$\cdot$}}}

\mode
<all>
